<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.SubScene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="609.0" style="-fx-background-color: #F2F2F2;"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.Visbruker_Superbruker_Controller">
    <children>
        <ImageView fitHeight="410.0" fitWidth="618.0" layoutX="-7.0">
            <image>
                <Image url="@../img/background.jpg"/>
            </image>
        </ImageView>
        <TableView fx:id="tableView" editable="true" layoutY="114.0" prefHeight="286.0" prefWidth="611.0"
                   tableMenuButtonVisible="true"/>
        <Separator layoutY="65.0" prefHeight="3.0" prefWidth="610.0" style="-fx-background-color: black;"/>
        <ImageView fitHeight="149.0" fitWidth="200.0" layoutX="210.0" layoutY="-35.0">
            <image>
                <Image url="@../img/logo3.png"/>
            </image>
        </ImageView>
        <Button fx:id="btnLeggTil" layoutX="14.0" layoutY="68.0" mnemonicParsing="false"
                onAction="#On_Click_BtnVisKomponenterTilBrukeren" prefHeight="46.0" prefWidth="177.0"
                style="-fx-background-color: #3daee4;" text="Vis komponenter til brukeren">
            <font>
                <Font name="Helvetica" size="12.0"/>
            </font>
        </Button>
        <Button fx:id="btnFjern" layoutX="222.0" layoutY="68.0" mnemonicParsing="false"
                onAction="#On_Click_BtnFjernBruker" prefHeight="46.0" prefWidth="177.0"
                style="-fx-background-color: #3daee4;" text="Fjern bruker">
            <font>
                <Font name="Helvetica" size="15.0"/>
            </font>
        </Button>
        <Button fx:id="btnRediger" layoutX="423.0" layoutY="68.0" mnemonicParsing="false"
                onAction="#On_Click_BtnRedigerBruker" prefHeight="46.0" prefWidth="177.0"
                style="-fx-background-color: #3daee4;" text="Rediger bruker">
            <font>
                <Font name="Helvetica" size="15.0"/>
            </font>
        </Button>
        <Button fx:id="btnTilbake" layoutX="534.0" layoutY="11.0" mnemonicParsing="false"
                onAction="#On_Click_BtnTilbake" style="-fx-background-color: black;" text="Tilbake" textFill="WHITE">
            <font>
                <Font name="Helvetica" size="14.0"/>
            </font>
        </Button>
        <SubScene fx:id="LeggTilKomponent_sub" height="286.0" layoutY="113.0" visible="false" width="610.0">
            <root>
                <Region/>
            </root>
        </SubScene>
        <AnchorPane fx:id="LeggTilKomponent_pane" layoutY="113.0" prefHeight="286.0" prefWidth="610.0" visible="false"/>
    </children>
</AnchorPane>
